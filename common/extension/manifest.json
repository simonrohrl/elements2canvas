{
    "manifest_version": 3,
    "name": "DOM Tracing Injector",
    "version": "1.0",
    "description": "Injects JS into every page for DOM tracing",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["inject.js"],
        "run_at": "document_start",
        "all_frames": true
      }
    ]
}